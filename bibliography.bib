@article{Rumsey:1988ie,
author = {Rumsey, William L and VANDERKOOI, J M and Wilson, David F. and Vanderkooi, Jane M},
title = {{Imaging of Phosphorescence - a Novel Method for Measuring Oxygen Distribution in Perfused Tissue}},
